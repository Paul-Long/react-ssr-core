{
  "name": "react-ssr-core",
  "version": "1.0.0",
  "description": "react ssr redux react-router",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "hot:start": "supervisor -w dist index.js",
    "hot:client": "better-npm-run hot:client",
    "hot:server": "better-npm-run hot:server",
    "build:client": "better-npm-run build:client",
    "build:server": "better-npm-run build:server"
  },
  "betterScripts": {
    "hot:client": {
      "command": "node ./src/server/hot.client.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "hot:server": {
      "command": "node ./src/server/hot.server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:client": {
      "command": "webpack --config ./webpack/webpack.client.config.js --display-modules",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:server": {
      "command": "webpack --config ./webpack/webpack.server.config.js --display-modules",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Paul-Long/react-ssr-core.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Paul-Long/react-ssr-core/issues"
  },
  "homepage": "https://github.com/Paul-Long/react-ssr-core#readme",
  "devDependencies": {
    "antd": "^3.8.2",
    "autoprefixer": "^9.1.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.1",
    "blueimp-md5": "^2.10.0",
    "classnames": "^2.2.6",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "echarts": "^4.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "history": "^4.7.2",
    "ignore-loader": "^0.1.2",
    "invariant": "^2.2.4",
    "isomorphic-fetch": "^2.2.1",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "postcss-less": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "style-loader": "^0.22.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "asset-require-hook": "^1.2.0",
    "css-modules-require-hook": "^4.2.3",
    "express": "^4.16.3",
    "express-cluster": "0.0.5",
    "http-proxy-middleware": "^0.18.0",
    "log4js": "^3.0.5",
    "supervisor": "^0.12.0"
  }
}
